  "license": "proprietary",
  "type": "project",
  "authors": [
    {
      "name": "Zahar Pylypchuck",
      "email": "zacharpu2@gmail.com"
    }
  ],
  "require": {
    "ext-curl": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-mbstring": "*",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "ext-zlib": "*",
    "matthiasmullie/minify": "^1.3",
    "nikic/fast-route": "^1.3",
    "pimple/pimple": "^3.2",
    "vlucas/phpdotenv": "^5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "8",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Src\\": "src/"
    },
    "classmap": [
      "src/"
    ]
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
